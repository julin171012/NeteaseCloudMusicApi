{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "https://github.com/julin171012/NeteaseCloudMusicApi/:match*"
    }
  ]
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
